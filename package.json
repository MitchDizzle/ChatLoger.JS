{
  "name": "ChatLoggerJS",
  "productName": "ChatLoggerJS",
  "version": "1.1.0",
  "description": "A node.js based steam chat logger.",
  "main": "src/main.js",
  "scripts": {
    "start": "npm run pre-start && electron-forge start",
    "package": "npm run pre-start && electron-forge package",
    "make": "npm run pre-start && electron-forge make",
    "publish": "npm run pre-start && electron-forge publish",
    "lint": "eslint src --color",
    "pre-start": "node prebuild.js"
  },
  "keywords": [],
  "author": "Mitchell Gardner",
  "license": "MIT",
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "npm",
        "icon": "src/icons/app.ico",
        "ignore": [
          ".gitignore",
          "logdata"
        ]
      },
      "electronWinstallerConfig": {
        "name": "ChatLoggerJS"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "MitchDizzle",
        "name": "ChatLogger.JS"
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "ChatLoggerJS"
      }
    }
  },
  "dependencies": {
    "electron-compile": "^6.4.4",
    "electron-prompt": "^1.3.0",
    "electron-squirrel-startup": "^1.0.0",
    "keytar": "^4.4.0",
    "moment": "^2.24.0",
    "steam-user": "^4.2.0"
  },
  "devDependencies": {
    "showdown": "^1.9.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "electron-forge": "^5.2.4",
    "electron-prebuilt-compile": "4.0.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.12.4"
  }
}
